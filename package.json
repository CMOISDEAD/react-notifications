{
  "name": "doom-react-notifications",
  "private": false,
  "version": "0.1.5",
  "description": "A simple react notification system",
  "author": "camilodavila",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "notification",
    "JavaScript"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/doom-react-notifications.umd.js",
  "module": "./dist/doom-react-notifications.es.js",
  "exports": {
    ".": {
      "import": "./dist/doom-react-notifications.es.js",
      "require": "./dist/doom-react-notifications.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
